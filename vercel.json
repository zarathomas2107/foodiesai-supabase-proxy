{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/auth/v1/(.*)",
      "dest": "/api/auth/$1"
    },
    {
      "src": "/rest/v1/(.*)",
      "dest": "/api/rest/$1"
    },
    {
      "src": "/storage/v1/(.*)",
      "dest": "/api/storage/$1"
    },
    {
      "src": "/realtime/v1/(.*)",
      "dest": "/api/realtime/$1"
    },
    {
      "src": "/functions/v1/(.*)",
      "dest": "/api/functions/$1"
    }
  ]
}